<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:layout_gravity="left"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="0dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="0dp"
            android:id="@+id/linearLayout" android:weightSum="1">
        <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?android:attr/selectableItemBackground"
                android:text="New Button"
                android:id="@+id/button" android:layout_weight="0.2"/>

        <Button android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="?android:attr/selectableItemBackground" android:text="New Button"
                android:id="@+id/button2" android:layout_weight="0.2"/>
        <Button android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="?android:attr/selectableItemBackground" android:text="New Button"
                android:id="@+id/button3" android:layout_weight="0.2"/>
        <Button android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="?android:attr/selectableItemBackground" android:text="New Button"
                android:id="@+id/button4" android:layout_weight="0.2"/>
        <Button android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="?android:attr/selectableItemBackground" android:text="New Button"
                android:id="@+id/button5" android:layout_weight="0.2"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/linearLayout"
    >
    </LinearLayout>
</RelativeLayout>

